{
  "extends": "airbnb",
  "globals": {
    "webpackConfig": false,
    "projectDir": true
  },
  "env": {
    "mocha": true,
    "node": true
  },
  "rules": {
    "space-in-parens": "off",
    "no-underscore-dangle": "off",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": [
      "warn",
      {
        "aspects": [
          "invalidHref"
        ]
      }
    ]
  }
}
